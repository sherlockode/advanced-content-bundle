<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="sherlockode_acb.field_type.text" class="Sherlockode\AdvancedContentBundle\FieldType\Text">
            <tag name="sherlockode_advanced_content.fieldtype"/>
        </service>
        <service id="sherlockode_acb.field_type.textarea" class="Sherlockode\AdvancedContentBundle\FieldType\TextArea">
            <tag name="sherlockode_advanced_content.fieldtype"/>
        </service>
        <service id="sherlockode_acb.field_type.wysiwyg" class="Sherlockode\AdvancedContentBundle\FieldType\Wysiwyg">
            <tag name="sherlockode_advanced_content.fieldtype"/>
        </service>
        <service id="sherlockode_acb.field_type.link" class="Sherlockode\AdvancedContentBundle\FieldType\Link">
            <tag name="sherlockode_advanced_content.fieldtype"/>
        </service>
        <service id="sherlockode_acb.field_type.iframe" class="Sherlockode\AdvancedContentBundle\FieldType\Iframe">
            <tag name="sherlockode_advanced_content.fieldtype"/>
        </service>
        <service id="sherlockode_acb.field_type.image" class="Sherlockode\AdvancedContentBundle\FieldType\Image">
            <tag name="sherlockode_advanced_content.fieldtype"/>
            <argument type="service" id="sherlockode_advanced_content.upload_manager"/>
            <argument type="service" id="assets.packages"/>
        </service>
        <service id="sherlockode_acb.field_type.file" class="Sherlockode\AdvancedContentBundle\FieldType\File">
            <tag name="sherlockode_advanced_content.fieldtype"/>
            <argument type="service" id="sherlockode_advanced_content.upload_manager"/>
            <argument type="service" id="assets.packages"/>
        </service>
        <service id="sherlockode_acb.field_type.relative_link" class="Sherlockode\AdvancedContentBundle\FieldType\RelativeLink">
            <tag name="sherlockode_advanced_content.fieldtype"/>
            <argument type="service" id="request_stack"/>
        </service>
        <service id="sherlockode_acb.field_type.separator" class="Sherlockode\AdvancedContentBundle\FieldType\Separator">
            <tag name="sherlockode_advanced_content.fieldtype"/>
        </service>
        <service id="sherlockode_acb.field_type.title" class="Sherlockode\AdvancedContentBundle\FieldType\Title">
            <tag name="sherlockode_advanced_content.fieldtype"/>
        </service>
        <service id="sherlockode_acb.field_type.video" class="Sherlockode\AdvancedContentBundle\FieldType\Video">
            <tag name="sherlockode_advanced_content.fieldtype"/>
        </service>
    </services>
</container>
